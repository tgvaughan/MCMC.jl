    <input spec='beast.evolution.tree.RandomTree' id='tree'>
        <input name='taxa' idref='alignment'/>
        <populationModel spec="ConstantPopulation">
            <popSize spec="beast.core.parameter.RealParameter" value="1.0"/>
        </populationModel>
    </input>

    <input spec='beast.core.parameter.RealParameter' id="popSize">1.0</input>


    <run spec="MCMC" id="mcmc" chainLength="1000000"> <!--autoOptimize="true"-->
        <state>
            <input name='stateNode' idref='tree'/>
        </state>

        <distribution spec="CompoundDistribution" id="posterior">
            <distribution id="coalescent" spec="Coalescent">
                <treeIntervals spec='TreeIntervals' id='TreeIntervals'>
                     <tree idref="tree"/>
                </treeIntervals>
                <populationModel spec="ConstantPopulation" id='ConstantPopulation'>
                    <parameter name="popSize" idref="popSize"/>
                </populationModel>
            </distribution>
        </distribution>

        <operator id='treeScaler' spec='ScaleOperator' scaleFactor="0.8" weight="1">
            <tree idref="tree"/>
        </operator>
        <operator id='treeScalerRoot' spec='ScaleOperator' scaleFactor="0.8" rootOnly="true" weight="1">
            <tree idref="tree"/>
        </operator>
        <operator spec='Uniform' weight="1">
            <tree idref="tree"/>
        </operator>
        <operator id='narrow' spec='Exchange' isNarrow='true' weight="1">
            <tree idref="tree"/>
        </operator>
        <operator id='wide' spec='Exchange' isNarrow='false' weight="1">
            <tree idref="tree"/>
        </operator>
        <operator spec='WilsonBalding' weight="1">
            <tree idref="tree"/>
        </operator>

        <logger logEvery="100" fileName="$(filebase).log">
            <!--parameter idref="treeModel.rootHeight"/-->
            <log id="tree_height" spec="beast.evolution.tree.TreeHeightLogger" tree="@tree"/>
            <log idref="popSize"/>
        </logger>
        <logger logEvery="1000" fileName="$(filebase).trees">
            <log idref="tree"/>
        </logger>
        <logger logEvery="100000">
            <log idref="tree_height"/>
            <log idref="popSize"/>
        </logger>
    </run>

</beast>
